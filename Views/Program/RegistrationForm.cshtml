@model StudentActivity.Models.Student_Program
@{
    ViewBag.Title = "RegistrationForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Registration Form</h2>


@using (Html.BeginForm("Save", "Program"))
{
    @Html.ValidationSummary()
    <div class="form-group">
        @Html.LabelFor(m => m.StudentId)
        @Html.TextBoxFor(m => m.StudentId, new { @class = "form-control" })
        @*Html.ValidationMessageFor(m => m.Customer.Name)*@
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Program.Title)
        @Html.TextBoxFor(m => m.ProgramId, new { @class = "form-control" })

    </div>

    @Html.HiddenFor(m => m.ProgramId)

    <button type="submit" class="btn btn-primary">
        Save
    </button>
}
